$(function(){
	var schoolId = "${schoolId}";
	if(schoolId==null || schoolId==""){
		schoolId = '${user.sysClass.schoolId}';
	}
	var gradeId = '${gradeId}';
	//获取周册数
	var week = '${week}';
	$("#week option[value='1']").removeAttr("selected");//根据值去除选中状态  
	$("#week option[value='"+week+"']").attr("selected","selected");//根据值让option选中 
	
	getAvg(schoolId,gradeId)//获取平均分(折线图)
	getRank(schoolId,gradeId)//获取排名(折线图)
	getSchInfo(schoolId,gradeId,week);//获取综合
	getSingleInfo(schoolId,gradeId,week);//获取单科
	
	getRateGrade(schoolId,gradeId,0);//获取年级的提交率(柱状图)
})
/**
 * 年级提交率
 */
//科目的切换
 $('.jiajiaiaj10').on('click',function(){
 	var index=$('.jiajiaiaj10').index(this);
 	$('.jiajiaiaj10').attr('id','')
 	$('.jiajiaiaj10').eq(index).attr('id','active')
 	$('.cscsh').hide();
 	$('.cscsh').eq(index).show();

 	var schoolId = "${schoolId}";
	if(schoolId==null || schoolId==""){
		schoolId = '${user.sysClass.schoolId}';
	}
	var gradeId = '${gradeId}';
	
	//获取班级提交率的柱状图
	getRateGrade(schoolId,gradeId,index)
 })
 
//获取年级的提交率(每周的提交率)
function getRateGrade(schoolId,gradeId,index){
	var city = [];//班级名称
 	var rate = [];//提交率
  	$.ajax({
  		url:'../schCount/getRateGrade.action',
  		data:{'schoolId':schoolId,'gradeId':gradeId,'index':index},
  		type:'post',
  		dataType:'json',
  		success:function(obj){
  			var data = obj.data;
  			for(var i=0;i<data.length;i++){
  				city[i]= data[i].week;
  				if(data[i].cityAvg > 0.9){
  					var obj = new Object;
  					obj.y = data[i].cityAvg*100;
  					obj.color = "#25bcef";
  					rate[i]=obj;
 				}else if(data[i].cityAvg > 0.6 && data[i].cityAvg < 0.9){
 					var obj = new Object;
  					obj.y = data[i].cityAvg*100;
  					obj.color = "#45ca59";
  					rate[i]=obj;
  				}else{
  					var obj = new Object;
  					obj.y = data[i].cityAvg*100;
  					obj.color = "#f8b551";
  					rate[i]=obj;
  				}
  			}
  			GradeRateHtml(city,rate);//页面显示数据
  		}
  	})
}

//提交率的柱状图
function GradeRateHtml(city,rate){
	$('#container10').highcharts({
		title: {
	        text: ''
	    },
	    credits:{
	    	enabled: false
	    },
        chart: {
            type: 'column'
        },
        xAxis: {
            categories: eval(city),
            crosshair: true
        },
        yAxis: {
        	tickPositions: [0, 25, 50, 75 , 100], // 指定竖轴坐标点的值
            min: 0,
            title: {
                text: '提交率'
            }
        },
        
        plotOptions: {
            column: {
                dataLabels: {
	                formatter: function() {
	                    return this.y.toFixed(1) + '%';
	                },
	                enabled: true
	            },
            }
        },
        series: [{
            name: '百分比',
            data: eval(rate),
            tooltip: {
	            pointFormat: '<span">{series.name}</span>: <b>{point.y}%</b> '
	        }
        }]
    });
}





//根据周测的选择,显示不同的周测数据
$(document).on("change","#week",function(){
	var schoolId = "${schoolId}";
	if(schoolId==null || schoolId==""){
		schoolId = '${user.sysClass.schoolId}';
	}
	//获取周册数
	var week = $("#week").val();
	var gradeId = '${gradeId}';
	
	getSchInfo(schoolId,gradeId,week);//获取综合
	getSingleInfo(schoolId,gradeId,week);//获取单科
})




//获取数量(综合)
function getSchInfo(schoolId,gradeId,week){
	$.ajax({
		url:'../schCount/getCountInfo.action',
		data:{'schoolId':schoolId,'gradeId':gradeId,'week':week},
		type:'get',
		dataType:'json',
		success:function(obj){
			if(obj.status){
				//获取综合数据
				var data = obj.data;
				$("#ZHList").empty();
				for(var i=0;i<data.length;i++){
					ZHHtml(data[i])
				}
				
			}
		}
	})
}

//获取数量(单科)
function getSingleInfo(schoolId,gradeId,week){
	$.ajax({
		url:'../schCount/getScoreInfo.action',
		data:{'schoolId':schoolId,'gradeId':gradeId,'week':week},
		type:'get',
		dataType:'json',
		success:function(obj){
			if(obj.status){
				//获取数学数据
				var MaData = obj.math;
				$("#MaList").empty();
				for(var i=0;i<MaData.length;i++){
					MaHtml(MaData[i])
				}
				//获取数学数据
				var EnData = obj.eng;
				$("#EnList").empty();
				for(var i=0;i<EnData.length;i++){
					EnHtml(EnData[i])
				}
				
			}
		}
	})
}
//数学数据的Html
function MaHtml(info){
	var avg = 0;
	if(info.referCount!=0){
		avg = info.totalScore/info.referCount;
	}
	var grAvg = 0;
	if(info.openCount!=0){
		grAvg = (info.grantCount/info.openCount)*100;
	}
	var reAvg = 0;
	if(info.openCount!=0){
		reAvg = (info.referCount/info.openCount)*100;
	}
	
	var html='<tr class="text-c">';
	 	html+=' <td>'+info.gradeName+info.className+'</td>';
		html+='	<td>'+info.teaName+'</td>';
		html+='	<td>'+info.grantCount+'</td>';
		html+='	<td>'+grAvg.toFixed(1)+'%</td>';
		html+='	<td>'+info.referCount+'</td>';
		html+='	<td>'+reAvg.toFixed(1)+'%</td>';
		html+='	<td>'+avg.toFixed(1)+'</td>';
		html+='	<td>'+info.cityAvg.toFixed(1)+'</td>';
		html+='	<td>'+info.schoolRank+'</td>';
		html+='	<td>'+info.cityRank+'</td>';
		html+='	<td><a href="../teaCount/teacherCount.action?gradeId='+info.gradeId+'&classId='+info.classId+'" target="_self" href="javascript:void(0)">查看</a></td>';
		html+='</tr>';
	$("#MaList").append(html);
}
