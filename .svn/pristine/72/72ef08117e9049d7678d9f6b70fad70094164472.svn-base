.panel{
	margin-bottom:15px
}
.border-grey{
	border:solid 1px #d5d5d5;
	position:relative
}
.border-blue{
	border:1px solid #57bdde;
	height:10vw;
	box-sizing:border-box;
	display:flex;
	flex-direction:column;
	justify-content:center
}
.border-blue .data_stage {
	margin-top:1%;
	margin-bottom:0;
	top:0
}
.border-blue .data_tit{
	position:relative
}
.border-blue .clearfix{
	display:flex;
	align-items:center
}
.img_box{
	display:flex;
	align-items:center;
	justify-content:center;
	border-right:#7fd6f2 1px solid
}
.data_panel{
	position:relative
}
.top15{
	margin:15px 0
}
.data_tit{
	position:absolute;
	right:0;
	top:0;
	height:calc(100% - 40px);
	display:flex;
	flex-direction:column;
	justify-content:center;
	font-size:16px;
}
.data_tit big{
	font-size:30px;
	font-weight:blod;
	margin-top:5px
}
.data_stage{
	margin-bottom:10px;
	top:-10px;
	height:40px;
	display:flex;
	justify-content:center;
	align-items:center;
	position:relative
}
.data_stage big{
	display:block;
	height:20px;
	border-left:2px solid #e0e0e0;
	position:absolute;
	left:50%;
	margin-left:-1px;
	top:10px
	
}
.data_stage span{
	display:block;
	width:50%;
	text-align:center;
	font-size:16px;
	color:#333
}
.right_panel{
	height:calc(20vw + 220px);
	display:flex;
	flex-direction:column;
	justify-content:space-between
}
.right_panel2{
	height:calc(12vw + 45px);
	display:flex;
	flex-direction:column;
	justify-content:space-between
}
.right_panel3{
	height:calc(10vw + 139px)
}
.right_panel3 .panel{
	height:calc(10vw + 139px)
}
.right_panel3 .panel-body{
	position:relative;
	height:calc(10vw + 53px);
	display:flex;
	align-items:center
}
.right_panel p{
	line-height:2em;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
	margin-bottom:0;
	font-size:18px
}
.right_panel .panel{
	position:relative;
}
.right_panel .right_panel_stage{
	position:absolute;
	height:100%;
	right:0;
	top:0;
	display:flex;
	align-items:center
}
.right_panel .right_panel_stage div{
	display:flex;
	flex-direction:column;
	font-size:22px;
	color:#8b8989
}
.right_panel .right_panel_stage font{
	color:#29aaf1;
	font-size:20px;
	margin-top:10px;
	display:flex;
	align-items:flex-end
}
.right_panel .right_panel_stage big{
	font-size:30px;
	font-weight:bold;
	display:inline-block;
	max-width:calc(100% - 60px);
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis;
}
.bar_box{
	width:100%;
	height:100%;
	display:flex;
	align-items:center
}
.bar_item{
	display:flex;
	flex-direction:column;
	justify-content:flex-end;
	align-items:center
}
.bar_total{
	width:4vw;
	height:10vw;
	display:flex;
	flex-direction:column;
	justify-content:flex-end
}
.bar_item p{
	line-height:2em;
	margin-top:0;
	font-size:18px
}
.bar_total .bar_actual{
	width:100%;
	position:relative;
}
.bar_total .bar_actual span{
	position:absolute;
	top:-1.5em;
	display:block;
	width:100%;
	text-align:center
}
.margin15{
	margin:15px
}
.long_s{
	width:210px;
}
.short_s{
	width:100px
}
.search_btn{
	height:23px;
	line-height:23px;
	border:0;
	padding:0 10px;
	vertical-align:top
}
.data1_item{
	height:50%;
	display:flex;
	flex-direction:column;
	justify-content:center
}
.data1_item .progress{
	width:100% !important
}
.data1_item .tit{
	display:flex;
	align-items:center;
	color:#949494;
	margin-bottom:5px
}
.data1_item .tit span{
	display:inline-block;
	width:3px;
	height:16px;
	background:#90dc64;
	margin-right:5px
}
.data1_item .num{
	font-size:12px;
	color:#7f807e;
	display:flex;
	align-items:center
}
.data1_item .num big{
	font-size:18px;
	color:#565656;
	margin:0;
	margin-right:5px;
	display:block;
	white-space:nowrap;
	max-width:calc(100% - 60px);
	overflow:hidden;
	text-overflow:ellipsis
}
.data1_item .num big.green{
	color:#90dc64
}
.data_panel1 .tit_chart{
	display:block;
	text-align:center;
	line-height:40px;
	font-size:16px;
	color:#333333;
	margin-bottom:10px;
	position:relative;
	top:-10px
}
.data_tit1{
	height:100%
}
.data1_item.data1_top{
	border-bottom:1px solid #dedede
}
.data-switch{
	position:absolute;
	top:5px;
	right:0
}
.data-switch a{
	font-size:14px;
	color:#333333;
	padding:2px 5px;
	margin:10px 5px;
	border-radius:10px
}
.data-switch a.active{
	background:#60d1fc;
	color:#fff;
}
.panel-header.flex{
	display:flex;
	align-items:center;
	justify-content:space-between
}