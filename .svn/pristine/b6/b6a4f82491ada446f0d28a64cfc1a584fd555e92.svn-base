<script type="text/javascript">
	$(function(){
		//getSchool();
		getArea();//获取区域信息
		loadOrder(1,20);
	})
	function loadOrder(page,rows,areaId,schoolId){
		$.ajax({
			url:'../contrast/getExamByPage.action',
			type:'get',
			data:{'page':page,'rows':rows,'areaId':areaId,'schoolId':schoolId},
			dataType:'json',
			success:function(obj){
				var data = obj.rows;
				var count = obj.total;
				$("#dataList").empty();
				$("#count").html(count);
				kkpagerHtml(count,rows,page,areaId,schoolId);
				if(data!=null){
					for(var i = 0;i<data.length;i++){
						dataListHtml(data[i]);
					}
				}
			}
		});
	}
	//回车按钮
	function keydownEvent() {
	     var e = window.event || arguments.callee.caller.arguments[0];
	     if (e && e.keyCode == 13 ) {
	     	document.getElementById("seach").click();
	     }
	};
	/**
	 * 点击搜索,
	 */
	 $("#seach").click(function(){
		var userNo = $("#userNo").val().trim();
		loadOrder(1,20,userNo);
	 });
	
	//根据区县查询班级和学生信息
	 $(document).on("change","#area",function(){
		var areaId= $("#area").val();
	 	var schoolId = $("#school").val();
	 	$("#school").empty();
		$("#school").append("<option value='' selected>请选择学校</option>")
		getSchool(areaId)//获取学校信息
	 	loadOrder(1,20,areaId,schoolId);
	 })
	 
	//根据学校查询班级和学生信息
	 $(document).on("change","#school",function(){
		var areaId= $("#area").val();
	 	var schoolId = $("#school").val();
	 	loadOrder(1,20,areaId,schoolId);
	 })
	
	 /**
	  * 学科列表Html
	  */
	 function dataListHtml(info){
		 var exam = info.exams;
		 var school = info.school;
		 var grade = info.grade;
		 var cla = info.sysClass;
		 
	 	 var html='';
	 	 html+='<tr class="text-c">';
	 	 html+='<td><input type="checkbox" value="'+info.userId+'" name="del"></td>';
	 	 html+='<td style="display:none">'+info.userId+'</td>';
	 	 html+='<td>'+info.userId+'</td>';
	 	 html+='<td>'+info.userName+'</td>';
	 	 html+='<td>'+info.userAccount+'</td>';
	 	 html+='<td>'+info.schoolId+'</td>';
	 	 html+='<td>'+grade.gradeName+'</td>';
	 	 html+='<td>'+info.classId+'</td>';
	 	 html+='<td>'+exam[0].ehschoolid+'</td>';
	 	 html+='<td>'+exam[0].ehgradeid+'</td>';
	 	 html+='<td>'+exam[0].ehclassid+'</td>';
	 	 
	 	 html+='<td class="td-manage">';
	 	 
	 	 html+='<a title="编辑" href="javascript:;" onclick="member_edit(&#39;订单修改&#39;,&#39;../contrast/updateExamView.action?userId='+info.userId+'&#39;,&#39;id&#39;,&#39;800&#39;,&#39;400&#39;)" class="ml-5" style="text-decoration:none"><i class="Hui-iconfont">&#xe6df;</i></a> ';
	 
	 	 html+='</td></tr>';
	 	 $("#dataList").append(html);
	 }
	 /**
	  * kkpager分页工具
	  */
	 function kkpagerHtml(sum,rows,current,areaId,schoolId){
	     var totalPage = Math.ceil(sum/rows);//总页数总数据条数/每页显示数 向上取整  
	     if(current == 0){  
	        current = 1;  
	     }  
	     //初始化函数  
	     kkpager.generPageHtml({  
	         pno : current, //当前页数  
	         total : totalPage,//总页数  
	         totalRecords : sum, //总数据条数  
	         mode:'click', //这里设置为click模式  
	         isShowTotalRecords:true,
	         //点击页码的函数，这里发送ajax请求后台  
	         click:function(n){
	        	 loadOrder(n,rows,areaId,schoolId);
	         	this.selectPage(n); //手动条用selectPage进行页码选中切换 
	         	return false;
	        },   
	 	},true);  
	 }
	 /**
	  * 将时间long类型转换为String类型
	  */
	  function formatterDateTime(val){
	     var date = new Date(val);
	     var y = date.getFullYear();  
	     var m = date.getMonth() + 1;  
	     m = m < 10 ? ('0' + m) : m;  
	     var d = date.getDate();  
	     d = d < 10 ? ('0' + d) : d;  
	     var h = date.getHours();  
	     h = h < 10 ? ('0' + h) : h;  
	     
	     var minute = date.getMinutes();  
	     minute = minute < 10 ? ('0' + minute) : minute; 
	     var second = date.getSeconds();
	     second = second < 10 ? ('0' + second) : second; 
	     return y + '-' + m + '-' + d+' '+ h+':'+minute+':'+second;  
	 }
	
</script>




<script type="text/javascript">
/*用户-添加*/
function member_add(title,url,w,h){
	layer_show(title,url,w,h);
}
/*用户-查看*/
function member_show(title,url,id,w,h){
	layer_show(title,url,w,h);
}
/*用户-编辑*/
function member_edit(title,url,id,w,h){
	layer_show(title,url,w,h);
}

/**
 * 批量修改异常数据
 */
 $("#delAll").click(function(){
	 var id = document.getElementsByName("del");
	 var ids = [];
	 for (var i=0;i<id.length;i++) {
		 if (id[i].checked == true) {
			 ids.push(id[i].value);
		 };
	 }
	 //调用删除方法
	 updateOrders(ids.join(","));
	 //alert(ids.join(","))
 });
 //批量修改异常订单
 function updateOrders(ids){
	 if (ids == "") {
	 	layer.msg("至少选中一条信息",{icon:7,time:1000});
	 } else {
		 layer.confirm('修改异常数据，确认要修改吗?',function(index){
		 $.ajax({
				url: '../contrast/updateExamByList.action',
				type: 'POST',
				data:{'ids':ids},
				dataType: 'json',
				traditional:true,
				success: function(data){
					if(data.status==true){
						layer.msg(data.msg,{icon:1,time:1000},function(){
							window.location.href="../contrast/examView.action";
						});
					}else{
						layer.msg(data.msg,{icon:2,time:1000},function(){
							window.location.href="../contrast/examView.action";
						});
					}
				},
				error: function(XmlHttpRequest, textStatus, errorThrown){
					layer.alert("系统出错",{icon:2},function(){
						window.location.href="../contrast/examView.action";
					});
				}
		     });
		 });
	 }
 }
 
 
 


//获取区域信息
 function getArea(){
 	$.ajax({
 		url:'../area/getAreaList.action',
 		type:'get',
 		dataType:'json',
 		success:function(obj){
 			if(obj.status==true){
 				var data = obj.data;
 				$("#area").empty();
 				$("#area").append("<option value='' selected>请选择</option>")
 				for(var i=0;i<data.length;i++){
 					$("#area").append("<option value='"+data[i].areaId+"'>"+data[i].areaname+"</option>")
 				}
 			}
 		},
 		error:function(){}
 	})
 }

//获取学校
function getSchool(areaId){
	$.ajax({
		url:'../school/getSchoolList.action',
		type:'get',
		data:{'areaId':areaId},
		dataType:'json',
		success:function(obj){
			if(obj.status==true){
				var data = obj.data;
				$("#school").empty();
				$("#school").append("<option value='' selected>请选择学校</option>")
				for(var i=0;i<data.length;i++){
					$("#school").append("<option value='"+data[i].schoolId+"'>"+data[i].schoolName+"</option>")
				}
			}
		},
		error:function(){}
	})
}

</script> 
