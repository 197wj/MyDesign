<script type="text/javascript">
//根据用户登录,判断显示什么页面
$(function(){
	var name = '${role.actorname}';
	var role = '${role.vchar2}'
	//超级管理员(所有东西都可以看)
	if(role=='superadmin'){
		$("#menu1").removeClass("hidden");
		$("#menu3").addClass("hidden");
	}
	//平台管理员(只能看到平台管理的数据统计)
	if(role=='platadmin'){
		$("#menu1").addClass("hidden");
		$("#menu3").removeClass("hidden");
		//控制显示连接
		$("#tea").addClass("hidden");
		$("#sch1").addClass("hidden");
		$("#sch2").addClass("hidden");
		$("#plt1").removeClass("hidden");
		$("#plt2").removeClass("hidden");
		//默认桌面显示页面
		$("#inde").empty();//桌面的默认页面(显示4年级)
		$("#inde").append('<iframe scrolling="yes" frameborder="0" src="${path }/platCount/platCount.action?gradeId=14"></iframe>')
	}
	//学校管理员(只能看到学校的数据统计)
	if(role=='schooladmin'){
		$("#menu1").addClass("hidden");
		$("#menu3").removeClass("hidden");
		//控制显示连接
		$("#tea").addClass("hidden");
		$("#plt1").addClass("hidden");
		$("#plt2").addClass("hidden");
		$("#sch1").removeClass("hidden");
		$("#sch2").removeClass("hidden");
		//默认桌面显示页面
		$("#inde").empty();//桌面的默认页面(显示4年级)
		$("#inde").append('<iframe scrolling="yes" frameborder="0" src="${path }/schCount/schoolCount.action?gradeId=14"></iframe>')
	}
	//教师和班级管理员(只能看到本班级的数据统计)
	if(role=='teacher'||role=='classadmin'){
		$("#menu1").addClass("hidden");
		$("#menu3").removeClass("hidden");
		//控制显示连接
		$("#sch1").addClass("hidden");
		$("#sch2").addClass("hidden");
		$("#plt1").addClass("hidden");
		$("#plt2").addClass("hidden");
		$("#tea").removeClass("hidden");
		//默认桌面显示页面
		$("#inde").empty();//桌面的默认页面(显示4年级)
		$("#inde").append('<iframe scrolling="yes" frameborder="0" src="${path }/teaCount/teacherCount.action"></iframe>')
	}
	
})
</script>

