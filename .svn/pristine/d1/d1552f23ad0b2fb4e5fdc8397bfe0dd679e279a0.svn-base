.barbox dd.barline{width:560px;background:#E3E3E3;height:20px;overflow:hidden;margin:8px 10px 0 20px;display:inline;border-bottom:solid 1px #F4F4F4;border-right:solid 1px #F4F4F4;border-radius:50px;}
.barbox dd.barline div.charts{height:20px;overflow:hidden;background:url("../../image/teacher/barbg.gif") repeat-x;background-size: cover;}
.barbox dd.barline div.charts.barred{background-position:0 0;border:1px solid #4AD537;background:#4AD537;background-size: cover;border-radius:50px;}
.attr2{background-position:0 0;background:#FF4444!important;background-size: cover;border-radius: 10px;}
.attr3{background-position:0 0;background:#fcc62e!important;background-size: cover;border-radius: 10px;}
.barbox dd.barline div.charts.baryellow{background-position:0 -10px;border:1px solid #4AD537;}
.barbox dd.barline div.charts.barblue{background-position:0 -20px;border:1px solid #4AD537;}
.mui-slider .mui-segmented-control.mui-segmented-control-inverted~.mui-slider-group .mui-slider-item{
	border: 0!important;
}
 